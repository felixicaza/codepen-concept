{
  "name": "codepen-concept",
  "version": "1.0.0",
  "private": true,
  "description": "Este repositorio contiene el desarrollo del dise√±o experimental de Codepen, creado por BlurSoul.",
  "keywords": [
    "astro",
    "design",
    "html",
    "css",
    "ui-design",
    "codepen",
    "redesign"
  ],
  "bugs": {
    "url": "https://github.com/felixicaza/codepen-concept/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixicaza/codepen-concept.git"
  },
  "license": "MIT",
  "author": "Felix Icaza <fx.joliett17@gmail.com>",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged && pnpm ls-lint",
    "commit-msg": "pnpm commitlint -c -e ${1}"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
