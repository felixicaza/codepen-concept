{
  "name": "codepen-concept",
  "version": "1.0.0",
  "private": true,
  "description": "Este repositorio contiene el desarrollo del dise√±o experimental de Codepen, creado por BlurSoul.",
  "keywords": [
    "astro",
    "design",
    "html",
    "css",
    "ui-design",
    "codepen",
    "redesign"
  ],
  "bugs": {
    "url": "https://github.com/felixicaza/codepen-concept/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixicaza/codepen-concept.git"
  },
  "license": "MIT",
  "author": "Felix Icaza <fx.joliett17@gmail.com>",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged && pnpm ls-lint",
    "commit-msg": "pnpm commitlint -c -e ${1}"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/ts-plugin": "1.10.4",
    "@fontsource-variable/montserrat": "5.2.5",
    "@playform/compress": "0.1.9",
    "@playform/inline": "0.1.2",
    "astro": "5.7.13",
    "astro-compressor": "1.0.0",
    "astro-icon": "1.1.5",
    "astro-seo-schema": "5.0.0",
    "astro-sitemap": "1.0.0",
    "astro-tunnel": "0.1.8",
    "lightningcss": "1.30.1",
    "sanitize.css": "13.0.0",
    "schema-dts": "1.1.5",
    "sharp": "0.34.1",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.27.0",
    "@ls-lint/ls-lint": "2.3.0",
    "astro-eslint-parser": "1.2.2",
