---
import type { Options } from '@contracts'
import Base from '@layouts/Base.astro'
import Navbar from '@components/Navbar.astro'
import Topbar from '@components/Topbar.astro'

interface Props {
  options: Options
}

const { options } = Astro.props
---

<Base options={options}>
  <Topbar />
  <Navbar />
  <slot />
</Base>
